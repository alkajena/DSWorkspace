package binaryTree;

public class PopulateInOrderSuccessor {
	Node root;
	
	static class Node{
		int key;
		Node left;
		Node right;
		Node next;
		
		public Node(int key){
			this.key=key;
			left=right=next=null;
		}
	}
	
	public PopulateInOrderSuccessor(int key){
		root=new Node(key);
	}
	
	public static void populate(Node root){
		if(root ==null)
			return;
		
		while(root!=null){
			root=root.right;
		}
		
			}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
